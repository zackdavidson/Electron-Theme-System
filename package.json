{
    "name": "electron-theme-system",
    "version": "1.0.0",
    "description": "This is just a module for creating a custom theme system, This will be implemented to Tabula Board",
    "main": "app/app.js",
    "scripts": {
        "start": "electron .",
        "build-all": "npm run build-win && npm run build-mac && npm run build-linux",
        "build-win": "electron-packager . --overwrite ETS --asar=true --platform=win32 --arch=ia32 --out=releases --prunt=true",
        "build-mac": "electron-packager . --overwrite ETS --platform=darwin --arch=x64 --out=releases --prunt=true",
        "build-linux": "electron-packager . --overwrite ETS --platform=linux --arch=x64 --out=releases --prunt=true"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/zackdavidson/Electron-Theme-System.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/zackdavidson/Electron-Theme-System/issues"
    },
    "homepage": "https://github.com/zackdavidson/Electron-Theme-System#readme",
    "dependencies": {
        "electron": "^1.6.10",
        "electron-storage": "^1.0.7"
    },
    "devDependencies": {
        "electron-packager": "^8.7.0"
    }
}